<h1 class="title">Manage Staff</h1>
<table class="table is-fullwidth">
    <tr>
        <th>Username</th>
        <th>Last login</th>
        <th>Last IP Address</th>
    </tr>
    {{#each data.users}}
    <tr>
        <td><a href="/admin/manage/user/{{this.id}}">{{ this.name }}</a></td>

        <td>{{timestampParseISO this.last_seen_time }}</td>

        <td>{{this.ip_address}}</td>
    </tr>
    {{/each}}
</table>